<?xml version="1.0" encoding="UTF-8"?>
<CONFIG>
  <ProjectOptions>
    <Version Value="12"/>
    <PathDelim Value="\"/>
    <General>
      <SessionStorage Value="InProjectDir"/>
      <Title Value="CS2ConfigWriterApp"/>
      <Scaled Value="True"/>
      <ResourceType Value="res"/>
      <UseXPManifest Value="True"/>
      <XPManifest>
        <DpiAware Value="True"/>
        <TextName Value="Free Software - CS2ConfigWriterApp"/>
        <TextDesc Value="App for modifying cs2 cfg file."/>
      </XPManifest>
      <Icon Value="0"/>
    </General>
    <BuildModes>
      <Item Name="Default" Default="True"/>
    </BuildModes>
    <PublishOptions>
      <Version Value="2"/>
      <UseFileFilters Value="True"/>
    </PublishOptions>
    <RunParams>
      <FormatVersion Value="2"/>
    </RunParams>
    <RequiredPackages>
      <Item>
        <PackageName Value="LCL"/>
      </Item>
    </RequiredPackages>
    <Units>
      <Unit>
        <Filename Value="cs2_cfg_writer_app.lpr"/>
        <IsPartOfProject Value="True"/>
      </Unit>
      <Unit>
        <Filename Value="umain.pas"/>
        <IsPartOfProject Value="True"/>
        <ComponentName Value="FormMain"/>
        <HasResources Value="True"/>
        <ResourceBaseClass Value="Form"/>
        <UnitName Value="uMain"/>
      </Unit>
      <Unit>
        <Filename Value="units\ucfgwriter.pas"/>
        <IsPartOfProject Value="True"/>
        <UnitName Value="uCfgWriter"/>
      </Unit>
      <Unit>
        <Filename Value="forms\usettingsform.pas"/>
        <IsPartOfProject Value="True"/>
        <ComponentName Value="SettingsForm"/>
        <HasResources Value="True"/>
        <ResourceBaseClass Value="Form"/>
        <UnitName Value="uSettingsForm"/>
      </Unit>
      <Unit>
        <Filename Value="units\ucfghinthandler.pas"/>
        <IsPartOfProject Value="True"/>
        <UnitName Value="uCfgHintHandler"/>
      </Unit>
      <Unit>
        <Filename Value="..\common\usettingswriter.pas"/>
        <IsPartOfProject Value="True"/>
        <UnitName Value="uSettingsWriter"/>
      </Unit>
      <Unit>
        <Filename Value="..\common\uglobalconstants.pas"/>
        <IsPartOfProject Value="True"/>
        <UnitName Value="uGlobalConstants"/>
      </Unit>
      <Unit>
        <Filename Value="forms\uviewmodelform.pas"/>
        <IsPartOfProject Value="True"/>
        <ComponentName Value="FormViewmodel"/>
        <HasResources Value="True"/>
        <ResourceBaseClass Value="Form"/>
        <UnitName Value="uViewmodelForm"/>
      </Unit>
      <Unit>
        <Filename Value="frames\uviewmodelframe.pas"/>
        <IsPartOfProject Value="True"/>
        <ComponentName Value="FrameViewmodel"/>
        <HasResources Value="True"/>
        <ResourceBaseClass Value="Frame"/>
        <UnitName Value="uViewmodelFrame"/>
      </Unit>
      <Unit>
        <Filename Value="forms\unewcfgwizardform.pas"/>
        <IsPartOfProject Value="True"/>
        <ComponentName Value="FormNewCfgWizard"/>
        <HasResources Value="True"/>
        <ResourceBaseClass Value="Form"/>
        <UnitName Value="uNewCfgWizardForm"/>
      </Unit>
      <Unit>
        <Filename Value="frames\ubindkeyframe.pas"/>
        <IsPartOfProject Value="True"/>
        <ComponentName Value="FrameBindKey"/>
        <HasResources Value="True"/>
        <ResourceBaseClass Value="Frame"/>
        <UnitName Value="uBindKeyFrame"/>
      </Unit>
      <Unit>
        <Filename Value="units\ucfgcommand.pas"/>
        <IsPartOfProject Value="True"/>
        <UnitName Value="uCfgCommand"/>
      </Unit>
      <Unit>
        <Filename Value="frames\ucommandlistframe.pas"/>
        <IsPartOfProject Value="True"/>
        <ComponentName Value="FrameCommandList"/>
        <ResourceBaseClass Value="Frame"/>
        <UnitName Value="uCommandListFrame"/>
      </Unit>
    </Units>
  </ProjectOptions>
  <CompilerOptions>
    <Version Value="11"/>
    <PathDelim Value="\"/>
    <Target>
      <Filename Value="$Env(OUTPUT_EXE)\cs2_cfg_writer_app"/>
    </Target>
    <SearchPaths>
      <IncludeFiles Value="$(ProjOutDir)"/>
      <OtherUnitFiles Value="units;forms;..\common;frames"/>
      <UnitOutputDirectory Value="$Env(OUTPUT_PPU)\lib\$(TargetCPU)-$(TargetOS)"/>
    </SearchPaths>
    <Linking>
      <Debugging>
        <DebugInfoType Value="dsDwarf2"/>
      </Debugging>
      <Options>
        <Win32>
          <GraphicApplication Value="True"/>
        </Win32>
      </Options>
    </Linking>
  </CompilerOptions>
  <Debugging>
    <Exceptions>
      <Item>
        <Name Value="EAbort"/>
      </Item>
      <Item>
        <Name Value="ECodetoolError"/>
      </Item>
      <Item>
        <Name Value="EFOpenError"/>
      </Item>
    </Exceptions>
  </Debugging>
</CONFIG>
